/*
 * QSPI Flash specific code
 */

&qspi {
      xlnx,fb-clk = <0x1>;
      xlnx,qspi-mode = <0x0>;
      flash0: s25fl128s@0 {
              compatible = "spansion,s25fl128s", "jedec,spi-nor";
              reg = <0x0>;
              spi-tx-bus-width = <1>;
              spi-rx-bus-width = <1>;
              spi-max-frequency = <40000000>;
              //spi-max-frequency = <100000000>;
              #address-cells = <1>;
              #size-cells = <1>;
              partition@qspi-fsbl-uboot {
                        label = "qspi-fsbl-uboot";
                        reg = <0x0 0x800000>;
              };              
      };
};
